<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="com.example.plugins.tutorial.add-workflow-extensions-tests" name="add-workflow-extensions" plugins-version="2">
  <plugin-info>
    <description>This is post function plugin for Jira</description>
    <version>1.0-SNAPSHOT</version>
    <vendor name="OBSS" url="http://www.obss.com.tr/"/>
  </plugin-info>
  <!-- from our base plugin -->
  <component-import key="myComponent" interface="com.example.plugins.tutorial.api.MyPluginComponent"/>
  <!-- from the product container -->
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>
  <component key="my-component-wired-test" class="it.com.example.plugins.tutorial.MyComponentWiredTest" public="true">
    <interface>it.com.example.plugins.tutorial.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
