<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jbosscmp-jdbc PUBLIC "-//JBoss//DTD JBOSSCMP-JDBC 4.0//EN" "http://www.jboss.org/j2ee/dtd/jbosscmp-jdbc_4_0.dtd">

<jbosscmp-jdbc>
   <defaults>
   </defaults>

   <enterprise-beans>

     <!--
       To add beans that you have deployment descriptor info for, add
       a file to your XDoclet merge directory called jbosscmp-jdbc-beans.xml
       that contains the <entity></entity> markup for those beans.
     -->

      <entity>
         <ejb-name>HistoryStepPrev</ejb-name>
         <table-name>OS_HISTORYSTEP_PREV</table-name>

         <cmp-field>
            <field-name>id</field-name>
            <column-name>ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>previousId</field-name>
            <column-name>PREVIOUS_ID</column-name>

        </cmp-field>

<!-- jboss 3.2 features -->
<!-- optimistic locking does not express the exclusions needed -->
      </entity>

      <entity>
         <ejb-name>HistoryStep</ejb-name>
         <table-name>OS_HISTORYSTEP</table-name>

         <cmp-field>
            <field-name>actionId</field-name>
            <column-name>ACTION_ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>caller</field-name>
            <column-name>CALLER</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>dueDate</field-name>
            <column-name>DUE_DATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>entryId</field-name>
            <column-name>ENTRY_ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>finishDate</field-name>
            <column-name>FINISH_DATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>id</field-name>
            <column-name>ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>owner</field-name>
            <column-name>OWNER</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>startDate</field-name>
            <column-name>START_DATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>status</field-name>
            <column-name>STATUS</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>stepId</field-name>
            <column-name>STEP_ID</column-name>

        </cmp-field>

<!-- jboss 3.2 features -->
<!-- optimistic locking does not express the exclusions needed -->
      </entity>

      <entity>
         <ejb-name>CurrentStepPrev</ejb-name>
         <table-name>OS_CURRENTSTEP_PREV</table-name>

         <cmp-field>
            <field-name>id</field-name>
            <column-name>ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>previousId</field-name>
            <column-name>PREVIOUS_ID</column-name>

        </cmp-field>

<!-- jboss 3.2 features -->
<!-- optimistic locking does not express the exclusions needed -->
      </entity>

      <entity>
         <ejb-name>WorkflowEntry</ejb-name>
         <table-name>OS_WFENTRY</table-name>

         <cmp-field>
            <field-name>id</field-name>
            <column-name>ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>state</field-name>
            <column-name>STATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>workflowName</field-name>
            <column-name>NAME</column-name>

        </cmp-field>

<!-- jboss 3.2 features -->
<!-- optimistic locking does not express the exclusions needed -->
      </entity>

      <entity>
         <ejb-name>CurrentStep</ejb-name>
         <table-name>OS_CURRENTSTEP</table-name>

         <cmp-field>
            <field-name>actionId</field-name>
            <column-name>ACTION_ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>caller</field-name>
            <column-name>CALLER</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>dueDate</field-name>
            <column-name>DUE_DATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>entryId</field-name>
            <column-name>ENTRY_ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>finishDate</field-name>
            <column-name>FINISH_DATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>id</field-name>
            <column-name>ID</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>owner</field-name>
            <column-name>OWNER</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>startDate</field-name>
            <column-name>START_DATE</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>status</field-name>
            <column-name>STATUS</column-name>

        </cmp-field>
         <cmp-field>
            <field-name>stepId</field-name>
            <column-name>STEP_ID</column-name>

        </cmp-field>

<!-- jboss 3.2 features -->
<!-- optimistic locking does not express the exclusions needed -->
      </entity>

   </enterprise-beans>

</jbosscmp-jdbc>
